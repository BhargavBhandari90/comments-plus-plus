{
	"name": "comments-plus-plus",
	"version": "1.0.0",
	"description": "A different experience of commenting.",
	"scripts": {
		"build": "wp-scripts build ./assets/js/main.js --output-path=./build",
		"format": "wp-scripts format",
		"lint:css": "wp-scripts lint-style",
		"lint:js": "wp-scripts lint-js",
		"packages-update": "wp-scripts packages-update",
		"plugin-zip": "wp-scripts plugin-zip",
		"makepot": "wp i18n make-pot . languages/comments-plus-plus.pot --exclude=node_modules,vendor,tests && echo '.pot file updated'",
		"start": "wp-scripts start ./assets/js/main.js --output-path=./build"
	},
	"keywords": [
		"commenting",
		"comments",
		"auto-complete"
	],
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"@wordpress/scripts": "^30.22.0"
	}
}
